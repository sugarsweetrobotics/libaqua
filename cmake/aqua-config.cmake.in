# libaqua CMake config file
#
# This file sets the following variables:
# libaqua_FOUND - Always TRUE.
# libaqua_INCLUDE_DIRS - Directories containing the libaqua include files.
# libaqua_IDL_DIRS - Directories containing the libaqua IDL files.
# libaqua_LIBRARIES - Libraries needed to use libaqua.
# libaqua_DEFINITIONS - Compiler flags for libaqua.
# libaqua_VERSION - The version of libaqua found.
# libaqua_VERSION_MAJOR - The major version of libaqua found.
# libaqua_VERSION_MINOR - The minor version of libaqua found.
# libaqua_VERSION_REVISION - The revision version of libaqua found.
# libaqua_VERSION_CANDIDATE - The candidate version of libaqua found.

message(STATUS "Found aqua-@PROJECT_VERSION@")
set(aqua_FOUND TRUE)

set(aqua_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME@-@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@"
    )

if(WIN32)
    set(aqua_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        )
else(WIN32)
    set(aqua_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        )
endif(WIN32)

set(aqua_VERSION @PROJECT_VERSION@)
set(aqua_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(aqua_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(aqua_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(aqua_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

